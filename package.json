{
	"name": "react-starter",
	"version": "1.0.0",
	"description": "react starter",
	"main": "index.tsx",
	"scripts": {
		"dev": "node ./webpack/webpack.dev-server.js",
		"start": "npm run build && node webpack/static-server.js",
		"build": "node ./node_modules/webpack/bin/webpack --config webpack/webpack.production.config.js",
		"test": "jest --watchAll=false",
		"test:watch": "jest --watchAll=false --watch",
		"test:coverage": "jest --coverage",
		"prettier": "prettier --write .",
		"lint": "eslint --debug .",
		"lint:write": "eslint --debug . --fix"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.(js|jsx|ts|tsx)": [
			"npm run lint:write",
			"git add -A"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Artem-Nanavov/react-starter.git"
	},
	"keywords": [
		"react"
	],
	"author": "Artem_Nanavov",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Artem-Nanavov/react-starter/issues"
	},
	"homepage": "https://github.com/Artem-Nanavov/react-starter#readme",
